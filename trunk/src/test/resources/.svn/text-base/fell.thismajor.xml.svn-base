    <pb n="1"/>
    <milestone n="A1r" unit="sig"/>
    <add resp="#lwalton.nen" hand="#unknown.zzx" rend="place(headspace)">
      <gap resp="#lwalton.nen" desc="handwriting" reason="illegible"/>
